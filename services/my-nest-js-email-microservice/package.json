{
	"name": "@my-projects-monorepo/my-nest-js-email-microservice",
	"version": "0.0.1",
	"private": false,
	"scripts": {
		"build": "nx build my-nest-js-email-microservice",
		"docker:build": "docker build -t my-nest-js-email-microservice-image -f Dockerfile ../../"
	},
	"nx": {
		"targets": {
			"build": {
				"executor": "nx:run-commands",
				"options": {
					"command": "webpack-cli build",
					"args": [
						"--node-env=production"
					],
					"cwd": "services/my-nest-js-email-microservice"
				},
				"configurations": {
					"development": {
						"args": [
							"--node-env=development"
						]
					}
				}
			},
			"prune-lockfile": {
				"dependsOn": [
					"build"
				],
				"cache": true,
				"executor": "@nx/js:prune-lockfile",
				"outputs": [
					"{workspaceRoot}/services/my-nest-js-email-microservice/dist/package.json",
					"{workspaceRoot}/services/my-nest-js-email-microservice/dist/pnpm-lock.yaml"
				],
				"options": {
					"buildTarget": "build"
				}
			},
			"copy-workspace-modules": {
				"dependsOn": [
					"build"
				],
				"cache": true,
				"outputs": [
					"{workspaceRoot}/services/my-nest-js-email-microservice/dist/workspace_modules"
				],
				"executor": "@nx/js:copy-workspace-modules",
				"options": {
					"buildTarget": "build"
				}
			},
			"prune": {
				"dependsOn": [
					"prune-lockfile",
					"copy-workspace-modules"
				],
				"executor": "nx:noop"
			},
			"serve": {
				"continuous": true,
				"executor": "@nx/js:node",
				"defaultConfiguration": "development",
				"dependsOn": [
					"build"
				],
				"options": {
					"buildTarget": "@my-projects-monorepo/my-nest-js-email-microservice:build",
					"runBuildTargetDependencies": false
				},
				"configurations": {
					"development": {
						"buildTarget": "@my-projects-monorepo/my-nest-js-email-microservice:build:development"
					},
					"production": {
						"buildTarget": "@my-projects-monorepo/my-nest-js-email-microservice:build:production"
					}
				}
			},
			"test": {
				"options": {
					"passWithNoTests": true
				}
			}
		}
	},
	"dependencies": {}
}
