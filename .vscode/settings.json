{
  "chat.agent.thinkingStyle": "expanded",
  "chat.agentSessionsViewLocation": "view",
  "chat.editor.fontFamily": "Fira Code, monospace",
  "chat.edits2.enabled": true,
  "chat.fontFamily": "Fira Code, monospace",
  "powershell.powerShellDefaultVersion": "PowerShell (x64)",
  "jest.runMode": "watch",
  "jest.rootPath": ".",
  "jest.jestCommandLine": "npx nx test",
  "wallaby.codeLensFeature.runTest": true,
  "markdown.occurrencesHighlight.enabled": true,
  "markdown.preview.fontFamily": "Fira Code, monospace",
  "markdown.validate.enabled": true,
  "markdown.extension.completion.enabled": true,
  "markdown.extension.completion.root": "./",
  "markdown.extension.list.indentationSize": "inherit",
  "markdown.extension.preview.autoShowPreviewToSide": false,
  "markdown.extension.print.theme": "light",
  "markdown.extension.showActionButtons": true,
  "markdown.extension.tableFormatter.normalizeIndentation": true,
  "editor.wordWrap": "on",
  "editor.formatOnSave": true,
  "editor.fontFamily": "Fira Code, monospace",
  "editor.tabSize": 2,
  "editor.formatOnPaste": true,
  "editor.aiStats.enabled": true,
  "editor.allowVariableFonts": true,
  "editor.allowVariableFontsInAccessibilityMode": true,
  "editor.allowVariableLineHeights": true,
  "editor.autoIndent": "full",
  "editor.autoIndentOnPaste": true,
  "editor.autoIndentOnPasteWithinString": true,
  "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
  "editor.bracketPairColorization.enabled": true,
  "editor.codeLensFontFamily": "Fira Code, monospace",
  "editor.colorDecorators": true,
  "editor.comments.insertSpace": true,
  "editor.defaultFormatter": null,
  "editor.definitionLinkOpensInPeek": true,
  "editor.detectIndentation": true,
  "editor.experimentalGpuAcceleration": "off",
  "editor.folding": true,
  "editor.foldingHighlight": true,
  "editor.foldingImportsByDefault": false,
  "editor.guides.indentation": true,
  "editor.hover.delay": 200,
  "editor.inlayHints.enabled": "on",
  "editor.inlayHints.fontFamily": "Fira Code, monospace",
  "editor.largeFileOptimizations": false,
  "editor.insertSpaces": true,
  "editor.lineNumbers": "on",
  "editor.linkedEditing": true,
  "editor.links": true,
  "editor.parameterHints.enabled": true,
  "editor.renderFinalNewline": "dimmed",
  "editor.unfoldOnClickAfterEndOfLine": true,
  "editor.wrapOnEscapedLineFeeds": true,
  "editor.formatOnType": true,
  "diffEditor.codeLens": true,
  "terminal.integrated.suggest.enabled": true,
  "[instructions]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.unicodeHighlight.ambiguousCharacters": false,
    "editor.unicodeHighlight.invisibleCharacters": false,
    "diffEditor.ignoreTrimWhitespace": false,
    "editor.wordWrap": "on",
    "editor.quickSuggestions": {
      "comments": "off",
      "strings": "off",
      "other": "off"
    }
  },
  "github.copilot.nextEditSuggestions.enabled": true,
  "chat.mcp.gallery.enabled": true,
  "chat.useNestedAgentsMdFiles": true,
  "markdownlint.config": {
    "ol-prefix": false
  },
  "terminal.integrated.defaultProfile.windows": "Git Bash",
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "codacy.guardrails.instructionsFile": "manual",
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "npm.enableRunFromFolder": true,
  "npm.packageManager": "pnpm",
  "npm.scriptRunner": "pnpm",
  "terminal.integrated.enableVisualBell": true,
  "terminal.integrated.environmentChangesIndicator": "on",
  "terminal.integrated.environmentChangesRelaunch": true,
  "terminal.integrated.fontFamily": "Fira Code, monospace",
  "terminal.integrated.fontLigatures.enabled": true,
  "terminal.integrated.shellIntegration.enabled": true,
  "cSpell.words": ["Chatmode", "chatmodes", "Codacy", "commitlint", "dtos", "emperorrag", "mermaidchart", "runbook"],
  "inlineChat.enableV2": true,
  "chat.useCloudButtonV2": true,
  "github.copilot.chat.scopeSelection": true,
  "github.copilot.chat.customInstructionsInSystemMessage": true,
  "github.copilot.chat.codeGeneration.useInstructionFiles": true,
  "github.copilot.chat.reviewAgent.enabled": true,
  "github.copilot.chat.reviewSelection.enabled": true,
  "github.copilot.chat.reviewSelection.instructions": [],
  "github.copilot.chat.pullRequestDescriptionGeneration.instructions": [],
  "github.copilot.chat.commitMessageGeneration.instructions": [],
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "workbench.editor.highlightModifiedTabs": true,
  "workbench.editor.pinnedTabsOnSeparateRow": true,
  "workbench.editor.wrapTabs": true,
  "workbench.editor.showTabIndex": true,
  "workbench.editor.limit.enabled": true,
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[yaml]": {
    "editor.defaultFormatter": "redhat.vscode-yaml"
  },
  "editor.indentSize": "tabSize",
  "editor.quickSuggestions": {
    "comments": "inline",
    "strings": "inline"
  }
}
